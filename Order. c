         printf("3.Food item-Pasta\n,
                    Price -179");
         break;
  case'4':
         printf("4.Food item-French Fries\n,
                    Price -99");
         break;
  case'5':
          ("3.Food item-Sandwitch\n
                    Price -149");
         break;
