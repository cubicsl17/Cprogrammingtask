#include<stdio.h>

int main()
{
  int marks;
  printf("Enter student's marks:\n");
  scanf ("%d",&marks); 
  if(85>=marks>=100)
  {
     printf("Grade A");
  }
  else if(70>=marks>=84)
  {
     printf("Grade B");
  }
  else if(55>=marks>=69)
  {
     printf("Grade C");
  }
  else if(40>=marks>=54)
  {
     printf("Grade D");
  }
  else
  {
     printf("Grade F");
  }
  return  0;
  }
